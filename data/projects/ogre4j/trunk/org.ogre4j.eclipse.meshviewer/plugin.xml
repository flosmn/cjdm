<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.ogre4j.eclipse.meshviewer.MeshViewer"
            extensions="mesh"
            icon="icons/ogrehead.gif"
            id="org.ogre4j.eclipse.meshviewer.MeshViewer"
            name="MeshViewer">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="OGRE"
            class="org.ogre4j.eclipse.meshviewer.OgreLogView"
            icon="icons/ogrehead.gif"
            id="org.ogre4j.eclipse.meshviewer.OgreLogView"
            name="OGRE Log">
      </view>
   </extension>
